<?xml version="1.0" encoding="UTF-8" ?>
<CycloneDDS xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns="https://cdds.io/config"
            xsi:schemaLocation="https://cdds.io/config https://cdds.io/config.xsd">
    <Domain id="any">
        <General>
            <Interfaces>
                <NetworkInterface autodetermine="true"/>
            </Interfaces>
            <AllowMulticast>false</AllowMulticast>
        </General>
        <Discovery>
            <Peers>
                <Peer address="10.32.196.149"/> <!-- PC IP -->
                <Peer address="10.32.196.64"/> <!-- RPi IP -->
            </Peers>
        </Discovery>
    </Domain>
</CycloneDDS>

